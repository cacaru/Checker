<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start="2dp"
        android:end="2dp"
        android:top="2dp"
        android:bottom="2dp">
        <shape  android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="?attr/customBorder"/>
            <solid android:color="?attr/customBorder"/>
        </shape>
    </item>
</selector>